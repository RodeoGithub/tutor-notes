<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maey.tutornotes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TutorNotes"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentHomeActivity"></activity>
        <activity
            android:name=".AddCourseActivity"
            android:parentActivityName=".CoursesListActivity"></activity>
        <activity
            android:name=".AddNoteActivity"
            android:parentActivityName=".NotesActivity"></activity>
        <activity android:name=".NotesActivity"></activity>
        <activity
            android:name=".CoursesListActivity"
            android:parentActivityName=".TutorHomeActivity"></activity>
        <activity android:name=".TutorHomeActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TypeActivity" />
    </application>

</manifest>